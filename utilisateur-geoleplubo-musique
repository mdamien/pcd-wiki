  \clef bass
  \key bes \major
  \time 4/4

   r4 (d g4 cis ~ cis d b d,)
}
    \new Staff = "lower" \relative c {
  \clef bass
  \key bes \major
  \time 4/4

   r1 r1
}
  >>
</score>


